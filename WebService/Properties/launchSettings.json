{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:54321/",
      "sslPort": 0
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "http://localhost:54321/v1/status",
      "environmentVariables": {
        "PCS_IOTHUB_CONNSTRING": "$(PCS_IOTHUB_CONNSTRING)"
      }
    },
    "WebService": {
      "commandName": "Project",
      "launchUrl": "http://localhost:54321/v1/status",
      "environmentVariables": {
        "PCS_IOTHUB_CONNSTRING": "$(PCS_IOTHUB_CONNSTRING)"
      },
      "applicationUrl": "http://localhost:54321/v1/status"
    }
  }
}